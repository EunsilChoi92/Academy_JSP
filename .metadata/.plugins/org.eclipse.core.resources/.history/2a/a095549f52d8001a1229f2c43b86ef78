		try {
			int i_board = Integer.parseInt(strI_board);
			return i_board;
		} catch(NumberFormatException e) {
			e.printStackTrace();
			return returnVal;
		}
	}
	
	
	public static void main(String[] args) {
		int num = parseStringToInt("하하", 5);
		System.out.println("경덕이 바보 : " + num);
	}
	
	
}
